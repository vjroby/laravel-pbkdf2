{
    "name": "vjroby/laravel-pbkdf2",
    "description": "This is a package for wrapping PBKDF2 class and using it in Laravel as a facade",
    "keywords": [
        "laravel","pbkdf2","hashing"
    ],
    "authors": [
        {
            "name": "Robert Gabriel Dinu",
            "email": "eu@robertgabriel.ro",
            "homepage":"http://robertgabriel.ro",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "3.7.*",
        "laravel/laravel": "4.2.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Vjroby\\LaravelPbkdf2\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "license": "MIT"
}
